EXTRA_DEPS += $(srcdir)/../objs.mk

ifeq (WINNT,$(OS_TARGET))
VISIBILITY_FLAGS =
endif

include $(srcdir)/../objs.mk


# Make a copy into the local directory for dual compilation
export:: $(MTRANSPORT_CPPSRCS)
	$(INSTALL) $^ .

# for stun.h
ifeq (WINNT,$(OS_TARGET))
DEFINES += \
  -DWIN32 \
  -DNOMINMAX \
  $(NULL)
else ifeq (Linux,$(OS_TARGET))
DEFINES += -DLINUX
endif
