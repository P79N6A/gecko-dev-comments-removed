include protocol PBackground;

namespace mozilla {
namespace dom {

// This protocol is used for the BroadcastChannel API
protocol PBroadcastChannel
{
  manager PBackground;

parent:
  PostMessage(nsString message);
  Close();

child:
  Notify(nsString message);
  __delete__();
};

} // namespace dom
} // namespace mozilla
